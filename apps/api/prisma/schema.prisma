generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Intent {
  id         String    @id @default(uuid())
  fullName   String
  email      String
  status     String    @default("PENDING")
  createdAt  DateTime  @default(now())
  
  @@map("intents")
}
